#include<stdio.h>
int main()
{
	int years;
	printf("enter the year:");
	scanf("%d",&years);
	if(years%400==0)
	printf("its the leap year");
	else if(years%100==0)
	printf("Its not a leap year");
	else if(years%4==0)
	printf("Its a leap year");
	else
	printf("The given year is not a leap year");
	return 0;
}
