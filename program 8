#include<stdio.h>
int main ()
{
	int total_user,staff_user,nonteaching_staff,student_user,users;
	printf("the total users:");
	scanf("%d",&total_user);
	printf("the number of staff users");
	scanf("%d",staff_user);
	nonteaching_staff=staff_user/3;
	users=nonteaching_staff+staff_user;
	student_user=total_user-users;
	printf("the number of student users is %d",student_user);
	return 0;
}
